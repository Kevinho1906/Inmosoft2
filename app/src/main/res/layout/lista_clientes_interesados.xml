<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/elementoCliente">


    <TextView
        android:id="@+id/txtNombreDelCliente"
        android:layout_width="295dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="t"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/txtApellidoDelCliente"
        android:layout_width="295dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="t"
        android:textAlignment="center"
        android:textSize="16sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtNombreDelCliente" />

    <TextView
        android:id="@+id/txtCorreoDelCLiente"
        android:layout_width="295dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="t"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtApellidoDelCliente" />

    <TextView
        android:id="@+id/txtTelefonoDelCLiente"
        android:layout_width="295dp"
        android:layout_height="36dp"
        android:layout_marginTop="16dp"
        android:text="t"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtCorreoDelCLiente" />

</androidx.constraintlayout.widget.ConstraintLayout>