<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:id="@+id/elementoProyecto"
    >


    <TextView
        android:id="@+id/txtNombreDelProyecto"
        android:layout_width="295dp"
        android:layout_height="36dp"
        android:text="t"
        android:textAlignment="center"
        android:textSize="24sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.039" />

    <TextView
        android:id="@+id/txtPrecio"
        android:layout_width="150dp"
        android:layout_height="40dp"
        android:text="t"
        android:textAlignment="center"
        android:textColor="#FF0000"
        android:textSize="16sp"
        android:textStyle="bold"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.476"
        app:layout_constraintStart_toEndOf="@+id/imgPerfilInicioSesion"
        app:layout_constraintTop_toBottomOf="@+id/txtUbicacion"
        app:layout_constraintVertical_bias="0.019" />

    <Button
        android:id="@+id/btnVerMas"
        style="?attr/materialButtonOutlinedStyle"
        android:layout_width="127dp"
        android:layout_height="48dp"
        android:backgroundTint="@color/white"
        android:drawableStart="@drawable/baseline_add_24"
        android:text="Ver Mas"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.491"
        app:layout_constraintStart_toEndOf="@+id/imgPerfilInicioSesion"
        app:layout_constraintTop_toBottomOf="@+id/txtPrecio"
        app:layout_constraintVertical_bias="0.032" />

    <TextView
        android:id="@+id/txtUbicacion"
        android:layout_width="206dp"
        android:layout_height="50dp"
        android:text="t"
        android:textAlignment="center"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.428"
        app:layout_constraintStart_toEndOf="@+id/imgPerfilInicioSesion"
        app:layout_constraintTop_toBottomOf="@+id/txtNombreDelProyecto"
        app:layout_constraintVertical_bias="0.035" />

    <ImageView
        android:id="@+id/imgPerfilInicioSesion"
        android:layout_width="152dp"
        android:layout_height="153dp"
        android:layout_marginStart="-150dp"
        android:layout_marginTop="-350dp"
        android:src="@drawable/perfil"
        android:translationZ="10dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.427"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.463" />


</androidx.constraintlayout.widget.ConstraintLayout>